PREFIX : <http://dbpedia.org/resource/> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX dbo: <http://dbpedia.org/ontology/> PREFIX foaf: <http://xmlns.com/foaf/0.1/> PREFIX dbp: <http://dbpedia.org/property/> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> SELECT DISTINCT ?label ?comment ?foundingYear ?revenue (GROUP_CONCAT(distinct ?product; separator=",") as ?products)  ?slogan (GROUP_CONCAT( distinct ?industry; separator=",") as ?industries)(GROUP_CONCAT( distinct ?ceo_entity; separator=",") as ?ceos)  (GROUP_CONCAT( distinct ?locationName; separator=",") as ?locationNames) ?numberOfEmployees ?companyFullName ?companyWebLink ?netIncome ?operatingIncome ?assets ?equity ?foundingDate (GROUP_CONCAT( distinct ?subsidiary; separator=",") as ?subsidiaries) ?areaServed ?dbpProducts WHERE { ?company rdfs:label ?label. ?company a dbo:Company. ?label <bif:contains> "${companyName}". OPTIONAL{?company rdfs:comment ?comment .} OPTIONAL{ ?company dbo:industry/rdfs:label ?industry .} OPTIONAL{?company dbo:foundingYear ?foundingYear.} OPTIONAL{?company dbo:location/rdfs:label ?locationName.} OPTIONAL{?company dbo:numberOfEmployees ?numberOfEmployees.} OPTIONAL{?company foaf:name ?companyFullName.} OPTIONAL{?company foaf:homepage ?companyWebLink.} OPTIONAL{ ?company dbo:netIncome ?netIncome.} OPTIONAL{?company dbo:operatingIncome ?operatingIncome.} OPTIONAL{?company dbo:revenue ?revenue.} OPTIONAL{?company dbo:keyPerson/foaf:name ?ceo_entity.} OPTIONAL{?company dbp:slogan ?slogan.} OPTIONAL{?company dbo:product/rdfs:label ?product.} OPTIONAL{?company dbo:assets ?assets.} OPTIONAL{?company dbo:equity ?equity.} OPTIONAL{?company dbo:foundingDate ?foundingDate.} OPTIONAL{?company dbo:subsidiary ?subsidiary.} OPTIONAL{?company dbp:areaServed ?areaServed.} OPTIONAL{?company dbp:products ?dbpProducts.} FILTER(LANG(?comment)="en" && LANG(?industry)="en" && LANG(?locationName)="en" && LANG(?companyFullName)="en" && LANG(?product)="en") } LIMIT 1